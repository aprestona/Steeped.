<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="sonsoflibertea.steeped">

<application
    android:allowBackup="true"
    android:icon="@drawable/green"
    android:label="@string/app_name"
    android:theme="@style/Theme.AppCompat" >
    <activity
        android:name=".SplashScreen"
        android:label="@string/title_activity_splash_screen" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".BrewType"
        android:label="@string/title_activity_brew_type" >
    </activity>
    <activity
        android:name=".StrengthDesire"
        android:label="@string/title_activity_strength_desire" >
    </activity>
    <activity
        android:name=".TeaType"
        android:label="@string/title_activity_tea_type" >
    </activity>
    <activity
        android:name=".Timer"
        android:label="@string/title_activity_timer" >
    </activity>
</application>

</manifest>